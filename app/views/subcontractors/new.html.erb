<%= provide(:title, 'New Subcontractor') %>

<div class="well">

  <div class="row-fluid">
      <div class="smart_label">
        <h1>New Subcontractor:</h1>
      </div>
  </div>
  <br>

  <div class="row-fluid">
    <div class="span5 ">
      <%= simple_form_for @subcontractor, url: subcontractors_url, :html => {:class => 'form-horizontal'} do |f| %>

          <%= render 'fields', f: f %>

          <div class="form-actions">
            <%= link_to 'Back', subcontractors_path, :class => 'btn btn-medium' %>
            <%= f.submit "Create Subcontractor", class: "btn btn-medium btn-primary", id: 'create_subcontractor_btn' %>
          </div>
      <% end %>

    </div>
  </div>
</div>


