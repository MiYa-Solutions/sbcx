<h1>Edit Subcontractor</h1>
<div class="row">
  <div class="span6 offset3">
    <%= simple_form_for @subcontractor, url: subcontractor_url(@subcontractor) do |f| %>
        <%= render 'fields', f: f %>
        <div>
          <%= f.submit "Create", class: "btn btn-large btn-primary" %>
        </div>
    <% end %>
  </div>
</div>

<p>
  <%= link_to "Show", @subcontractor %> |
  <%= link_to "View All", subcontractors_path %>
</p>
