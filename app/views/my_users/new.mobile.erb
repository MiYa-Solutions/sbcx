<% provide(:title, I18n.t('general.general.new').html_safe + "&nbsp;".html_safe + @my_user.class.human_attribute_name(:my_user) ) %>



<%= simple_form_for @my_user, url: my_users_path, :html => {:class => 'form-horizontal' } do |f| %>


    <fieldset class="ui-grid-a ui-btn-inner-small">
      <div class="ui-block-a">
        <%= link_to 'Back', my_users_path, "class" => "ui-btn-up-c", "data-role" => "button", "data-icon" => "arrow-l", "data-iconpos" => "left" %>
      </div>
      <div class="ui-block-b">
        <div class="miya_blue_button">
          <%= f.submit I18n.t('general.general.Save'), "class" => "ui-btn-up-b", "data-role" => "button", "data-theme" => "b", id: 'save_user_button', "data-icon" => "check", "data-iconpos" => "left" %>
        </div>
      </div>
    </fieldset>


    <% if f.object.errors.any? %>
        <%= render 'shared/error_messages', object: f.object %>
    <% end %>

    <%= render 'fields', f: f %>


    <div class="miya_blue_button">
      <%= f.submit I18n.t('general.general.Save'), "class" => "ui-btn-up-b", "data-role" => "button", "data-theme" => "b", id: 'create_user_button', "data-icon" => "check", "data-iconpos" => "left" %>
    </div>

<% end %>