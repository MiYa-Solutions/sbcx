<% provide(:title, I18n.t('general.general.new_project').html_safe ) %>

<%= simple_form_for @project, :html => { :class => 'form-horizontal' } do |f| %>
    <%= f.error_notification %>

    <%= render 'form', f: f %>

    <!--<div class="form-actions">-->
    <%= f.submit I18n.t('general.general.create_project'), id: 'service_call_create_btn', "data-theme" => "b" %>
    <%= link_to I18n.t('general.general.back'), projects_path, "data-role" => "button", :class => "ui-btn-up-d" %>

    <!--</div>-->
<% end %>
