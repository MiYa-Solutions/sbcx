<h1> Organization Attributes</h1>
<% for attribute in @organization.attributes.keys  %>
    <b><%= attribute.humanize %></b> <%= @organization.attributes[attribute].to_s %><br />
<% end %>

<h2> Organization Roles</h2>
<ul>
<% for role in @organization.organization_roles %>

    <li><%= role.name %></li>
</ul>
<% end %>
<p>

</p>
<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              organizations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_organization_path(@organization), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              organization_path(@organization),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>




