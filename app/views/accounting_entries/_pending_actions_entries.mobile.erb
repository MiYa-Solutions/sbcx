<div data-role="content">

  <div class="subcon_entries_page">
    <br>
    <ul data-role="listview">
      <li class="ui-li ui-li-static">
        <div class="ui-grid-a">
          <div class="ui-block-a"> <%= AccountingEntry.human_attribute_name(:id) %></div>
          <div class="ui-block-b"> <%= accounting_entry.id %></div>
        </div>
      </li>
      <li class="ui-li ui-li-static">
        <div class="ui-grid-a">
          <div class="ui-block-a"> <%= AccountingEntry.human_attribute_name(:type) %></div>
          <div class="ui-block-b"> <%= accounting_entry.class.model_name.human %></div>
        </div>
      </li>
      <li class="ui-li ui-li-static">
        <div class="ui-grid-a">
          <div class="ui-block-a"> <%= AccountingEntry.human_attribute_name(:status) %></div>
          <div class="ui-block-b"> <%= accounting_entry.human_status_name.humanize %></div>
        </div>
      </li>
      <li class="ui-li ui-li-static">
        <div class="ui-grid-a">
          <div class="ui-block-a"> <%= AccountingEntry.human_attribute_name(:collector) %></div>
          <div class="ui-block-b"> <%= accounting_entry.collector.name if defined? accounting_entry.collector %></div>
        </div>
      </li>
      <li class="ui-li ui-li-static">
        <div class="ui-grid-a">
          <div class="ui-block-a"> <%= AccountingEntry.human_attribute_name(:amount) %></div>
          <div class="ui-block-b"> <%= humanized_money_with_symbol accounting_entry.amount %></div>
        </div>
      </li>
      <li class="ui-li ui-li-static">
        <div class="ui-grid-a">
          <div class="ui-block-a"> <%= AccountingEntry.human_attribute_name(:notes) %></div>
          <div class="ui-block-b"> <%= accounting_entry.notes %></div>
        </div>
      </li>

    </ul>

    <br><br>


    <div class="service_call_entries_actions">

      <%= adjustment_entry_actions(accounting_entry, "customer_entry_small_btn") %>
      <!--<a href="#" data-role="button" data-inline="true" data-rel="back" data-transition="flow" data-theme="d">Close</a>-->
    </div>

  </div>
</div>