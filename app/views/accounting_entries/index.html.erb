<h1>Listing accounting_entries</h1>

<table>
  <tr>
    <th>Status</th>
    <th>Event</th>
    <th>Amount cents</th>
    <th>Amount currency</th>
    <th>Ticket</th>
    <th>Account</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @accounting_entries.each do |accounting_entry| %>
      <tr>
        <td><%= accounting_entry.status %></td>
        <td><%= accounting_entry.event_id %></td>
        <td><%= accounting_entry.amount_cents %></td>
        <td><%= accounting_entry.amount_currency %></td>
        <td><%= accounting_entry.ticket_id %></td>
        <td><%= accounting_entry.account_id %></td>
        <td><%= link_to 'Show', accounting_entry %></td>
        <td><%= link_to 'Edit', edit_accounting_entry_path(accounting_entry) %></td>
        <td><%= link_to 'Destroy', accounting_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
</table>

<br/>

<%= link_to 'New Accounting entry', new_accounting_entry_path %>
